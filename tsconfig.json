{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "",
		"outDir": "./dist",
		"sourceMap": true,
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "esnext",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
			"@framework/*": [
				"projects/plain/src/lib/framework/*"
			],
			"@model/*": [
				"projects/plain/src/lib/model/*"
			],
			"@plain/*": [
				"projects/plain/src/lib/*"
			]
		}
	}
}